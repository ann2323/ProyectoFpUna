	<queryString>
		<![CDATA[select cli.nombre||' '||cli.apellido as nombre, trim(to_char(cast(cli.cedula as integer),'9G999G999')) as cedula, recibo.nro_recibo, recibo.fecha  as rec_fecha,
det.nro_tarjeta_cheque , recibo.fecha_anulacion, pend.pagado, det.nombre_pago, pend.nro_factura
from cabecera_recibo recibo
join cliente cli on (recibo.cliente_id=cli.cliente_id)
join factura_pendiente pend on (recibo.recibo_id=pend.recibo_id)
join venta ven on (pend.nro_prefijo=ven.nro_prefijo and pend.nro_factura=ven.nro_factura)
join detalle_pago det on (pend.factura_pendiente_id=det.factura_pendiente_id)
where recibo.nro_recibo=$P{nroRecibo} and recibo.fecha_anulacion is null]]>
	</queryString>
