	<queryString>
		<![CDATA[select
 t0.codigo,
 t0. descripcion,
 t0.unidad,
 case t0.estado when 0 then 'Inactivo' when 1 then 'Activo' end as estado,
 t0.precio,
 t1.cod_proveedor || ' - ' || t1.nombre || ' ' || coalesce(apellido,'') as proveedor,
coalesce(t2.cod_deposito,'') as deposito,
coalesce(t2.cantidad,0) as cantidad
from componentes t0
left outer join proveedor t1 on t1.proveedor_id = t0.id_proveedor
left outer join stock t2 on t2.cod_componente = t0.codigo]]>
	</queryString>
